<h1>I am a login page!</h1>

<div class="row">
	<span class="large-3 large-centered columns">
		<%= form_for :user do |f| %>
			<%= f.email_field :email %>

			<%= f.password_field :password %>

			<%= f.submit "Log In" %>
		<% end %>

	<%= link_to "Create Account", new_user_path, :class => "button" %>

	</span>
</div>